<form class="bootstraper" @submit='c.Submit(event)'>
   <div class="form-group">
       <label for="node-count">Node Count</label>
       <input type="text" @change="c.SetNodeCount(event)" class="form-control" :value='c.NodeCount'/>
   </div>
   <button type="submit" class="btn btn-primary">Bootstrap!</button>
</form>
